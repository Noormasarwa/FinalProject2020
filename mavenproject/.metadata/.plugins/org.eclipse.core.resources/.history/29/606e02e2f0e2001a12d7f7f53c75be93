	/**
	 * Initialize the contents of the frame.
	 */
	public void switchPanel(JLayeredPane layeredPane,JPanel panel) {
		layeredPane.removeAll();
		layeredPane.add(panel);
		layeredPane.repaint();
		layeredPane.revalidate();
		
	}

